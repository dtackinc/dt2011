$(SRC)/test/high/dttcast.use: \
	  $(SRC)/test/high/dttcast.c
	bash dtinclude.sh \
	  -I$(SRC)/doc/include \
	  -I$(DT_YAM)/doc/include \
	  $(SRC)/test/high/dttcast.c | \
	bash dtdx.sh $(SRC) -fuse -o/tmp/dttcast \
	  "\"-aDavid Erb\"" \
	  "\"-cDtack, Inc.\"" -
	(echo "/*"; cat /tmp/dttcast.use; echo "*/") > $(SRC)/test/high/dttcast.use
	dtutil cstring usage < /tmp/dttcast.use >> $(SRC)/test/high/dttcast.use
	rm -f /tmp/dttcast.use



  


#----------------------------------------------------------------------------

examples: 


usages: \
	  $(SRC)/test/high/dttcast.use


#-------------------------------------------------------------------------
cis:
	find . -type d ! -name . -prune -o \
	  \( -name "*.[chx]" -o -name "dir.yam" \) \
	  -exec ci -l {} </dev/null \;

#-------------------------------------------------------------------------
maks: \
